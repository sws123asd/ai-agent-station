<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fun.wswj.ai.infrastructure.dao.AiRagOrderDao">
    <resultMap id="BaseResultMap" type="fun.wswj.ai.infrastructure.dao.po.AiClientRagOrder">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="rag_id" property="ragId" jdbcType="VARCHAR" />
        <result column="rag_name" property="ragName" jdbcType="VARCHAR" />
        <result column="knowledge_tag" property="knowledgeTag" jdbcType="VARCHAR" />
        <result column="status" property="status" jdbcType="TINYINT" />
        <result column="create_time" property="createTime" jdbcType="DATE" />
        <result column="update_time" property="updateTime" jdbcType="DATE" />
    </resultMap>
    <sql id="Base_Column_List">
        id, rag_id, rag_name, knowledge_tag, status, create_time, update_time
    </sql>
    <insert id="createTagOrder">
        INSERT INTO ai_client_rag_order (rag_id, rag_name, knowledge_tag)
        VALUES (#{ragId}, #{ragName}, #{knowledgeTag})
    </insert>
</mapper>